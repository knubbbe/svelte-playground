{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "@kazzkiq/svelte-preprocess-scss": "^0.1.3",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.66.2",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-svelte": "^4.3.1",
    "rollup-plugin-terser": "^3.0.0",
    "sirv-cli": "^0.2.2",
    "svelte": "^2.13.5"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p watch:scss start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "watch:scss": "node-sass -w  src/scss/app.scss --source-map true --output-style compress -o public/"
  },
  "dependencies": {
    "date-fns": "^1.29.0"
  }
}
